{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"title": "Update user schema",
	"description": "The schema for updating a user",
	"default": {},
	"examples": [
		{
			"username": "exampleUsername",
			"first_name": "exampleFirstName",
			"last_name": "exampleLastName",
			"email": "example@example.com"
		}
	],
	"required": [],
	"additionalProperties": true,
	"properties": {
		"username": {
			"$id": "#/properties/username",
			"type": "string",
			"title": "The username schema",
			"description": "username",
			"default": "",
			"examples": [ "exampleUsername" ]
		},
		"first_name": {
			"$id": "#/properties/first_name",
			"type": "string",
			"title": "The first_name schema",
			"description": "Users' first name",
			"default": "",
			"examples": [ "exampleFirstName" ],
			"maxLength": 30,
			"minLength": 1
		},
		"last_name": {
			"$id": "#/properties/last_name",
			"type": "string",
			"title": "The last_name schema",
			"description": "Users' last name",
			"default": "",
			"examples": [ "exampleLastName" ],
			"maxLength": 30,
			"minLength": 1
		},
		"email": {
			"$id": "#/properties/email",
			"type": "string",
			"title": "The email schema",
			"description": "Unique user email ",
			"default": "",
			"examples": [ "example@example.com" ]
		},
		"photo_url": {
			"$id": "#/properties/phoro_url",
			"type": "string",
			"title": "The photo_url schema",
			"description": "photo url for user",
			"default": "",
			"examples": [ "https://example.com/photo.png" ]
		}
	}
}
